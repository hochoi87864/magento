<?xml version="1.0"?>
<config>
	<modules>
		<Basetut_Helloworld>
			<version>0.1.0</version>
		</Basetut_Helloworld>
	</modules>
	<frontend>
		<routers>
			<Helloworld>
				<use>standard</use>
				<args>
					<module>Basetut_Helloworld</module>
					<frontName>helloworld</frontName>
				</args>
			</Helloworld>
		</routers>
		<layout>
			<updates>
				<helloworld>
					<file>helloworld.xml</file>
				</helloworld>
			</updates>
		</layout>
	</frontend>
	<global>
		<blocks>
			<helloworld>
				<class>Basetut_Helloworld_Block</class>
			</helloworld>
		</blocks>
	</global>
</config>